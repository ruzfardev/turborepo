    "effector": "^23.2.2",
    "effector-react": "^23.2.1",
    "lucide-react": "^0.474.0",
    "patronum": "^2.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-grid-layout": "^1.4.4",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@shared/eslint-config": "*",
    "@shared/typescript-config": "*",
