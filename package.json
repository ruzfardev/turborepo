    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.2.5",
    "turbo": "^2.0.3",
    "typescript": "^5.3.3"
  },
  "packageManager": "yarn@1.22.22",
  "engines": {
    "node": ">=18"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "packages/pages/*",
    "packages/widgets/*",
