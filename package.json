{
  "name": "turoprepo",
  "private": true,
  "scripts": {
    "dev": "turbo dev --concurrency=24",
    "dev:shell": "turbo dev --filter=shell",
    "build": "turbo build",
    "build:shell": "turbo build --filter=shell",
    "lint": "turbo lint",
    "lint:shell": "turbo lint --filter=shell",
    "graph": "turbo run build --graph=deps-graph.png",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
